<div class="container">
  <div class="row featurette detail">
    <div class="col-md-7">
      <h2 class="featurette-heading">{{model.heading}}</h2>
      <h3 class="text-muted">{{model.sub_heading}}</h3>
      <p class="lead">{{model.description}}</p>
    </div>

    <div class="col-md-5">
      <img class="featurette-image img-responsive center-block" alt={{model.alt}} src={{model.image}}>
    </div>
  </div>

  <hr class="featurette-divider">
</div>